{
  "name": "@m-mrz/route-gen",
  "type": "module",
  "version": "1.3.1",
  "description": "A file-based route generator for React Router",
  "repository": {
    "type": "git",
    "url": "https://github.com/mtt-merz/route-gen"
  },
  "bin": {
    "route-gen": "dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "check": "biome check --apply .",
    "dev": "concurrently \"sleep 2 && node dist/main.js\" \"pnpm run build\" --names node,build --prefix-colors blue,green",
    "postinstall": "pnpm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react-router",
    "codegen",
    "file-based"
  ],
  "author": "Matteo Merz",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "react-router": "^6.22.2",
    "typescript": "^5.3.3"
  }
}
